{
  "editor.wordWrap": "on",
  "[javascript|javascriptreact|typescriptreact|feature]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "json"
  ],
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.quickSuggestions": {
    "other": true,
    "comments": true,
    "strings": true
  },
  "turboConsoleLog.includeFileNameAndLineNum": false,
  "turboConsoleLog.insertEnclosingClass": false,
  "turboConsoleLog.quote": "`",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "typescript.validate.enable": true,
  "playwright.reuseBrowser": false,
  "eslint.format.enable": true,
  "window.autoDetectColorScheme": true,
  "workbench.preferredLightColorTheme": "Default Light+",
  "workbench.preferredDarkColorTheme": "Default Dark+",
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "liveshare.accessibility.soundsEnabled": false,
  "nxConsole.generateAiAgentRules": true,
  "files.associations": {
    "*.http": "http"
  },
  "[http]": {
    "editor.quickSuggestions": {
      "other": true,
      "comments": true,
      "strings": true
    }
  },
  "yaml.schemas": {
    "https://json.schemastore.org/openapi-3.0.json": [
      "tools/openapi/openapi.yaml"
    ],
    "https://json-schema.org/draft-07/schema": [
      "tools/openapi/components/**/*.yaml",
      "tools/openapi/paths/**/*.yaml"
    ]
  },
  "yaml.customTags": ["!include scalar"],
  "github.copilot.chat.customOAIModels": {
    "GLM-4.6": {
      "name": "GLM-4.6",
      "thinking": false,
      "url": "https://api.z.ai/api/coding/paas/v4",
      "toolCalling": true,
      "vision": false,
      "maxInputTokens": 128000,
      "maxOutputTokens": 8192,
      "requiresAPIKey": true
    }
  }
}
