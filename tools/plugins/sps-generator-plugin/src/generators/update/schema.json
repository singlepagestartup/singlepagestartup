{
  "$schema": "https://json-schema.org/schema",
  "$id": "Update",
  "title": "",
  "type": "object",
  "properties": {
    "module_name": {
      "type": "string",
      "description": "Module",
      "x-prompt": {
        "message": "Select module for creating model",
        "type": "list",
        "default": "sps-website-module",
        "items": [
          {
            "value": "sps-billing"
          },
          {
            "value": "sps-crm"
          },
          {
            "value": "sps-ecommerce"
          },
          {
            "value": "sps-file-storage"
          },
          {
            "value": "sps-rbac"
          },
          {
            "value": "sps-subscription"
          },
          {
            "value": "sps-website-builder"
          },
          {
            "value": "startup"
          }
        ]
      }
    },
    "right_external_models": {
      "type": "string",
      "description": "External models for relations, by space separated: sps-file-storage-widgets sps-file-storage-module-widgets. For example for 'hero-section-blocks-to-sps-file-storage-widgets' in 'sps-website-builder', the exernal model is 'sps-file-storage-widgets'.",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    },
    "left_external_models": {
      "type": "string",
      "description": "External models for relations, by space separated: sps-file-storage-widgets sps-file-storage-module-widgets",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    }
  },
  "required": ["module_name"]
}
