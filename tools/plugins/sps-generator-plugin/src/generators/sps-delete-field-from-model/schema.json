{
  "$schema": "https://json-schema.org/schema",
  "$id": "SpsDeleteFieldFromModel",
  "title": "",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "What name would you like to use?"
    },
    "model": {
      "type": "string",
      "description": "Model for adding field. It should be *-models-([\\w]+)-backend-app",
      "x-prompt": "Select model",
      "$default": {
        "$source": "project"
      },
      "x-dropdown": "projects"
    },
    "type": {
      "type": "string",
      "description": "Type of component",
      "x-prompt": {
        "message": "Select type of adding field",
        "type": "list",
        "default": "text",
        "items": [
          {
            "value": "text"
          },
          {
            "value": "number"
          }
        ]
      }
    },
    "level": {
      "type": "string",
      "description": "Where to place the field",
      "x-prompt": {
        "message": "Select level",
        "type": "list",
        "default": "startup",
        "items": [
          {
            "value": "startup"
          },
          {
            "value": "sps-lite"
          }
        ]
      }
    }
  },
  "required": ["name", "model", "type", "level"]
}
